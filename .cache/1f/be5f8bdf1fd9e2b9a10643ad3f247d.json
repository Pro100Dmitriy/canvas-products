{"line":84,"column":22}},{"generated":{"line":90,"column":13},"source":"assets/js/tooltip.js","original":{"line":84,"column":12}},{"generated":{"line":91,"column":0},"source":"assets/js/tooltip.js","original":{"line":85,"column":9}},{"generated":{"line":92,"column":0},"source":"assets/js/tooltip.js","original":{"line":59,"column":11}},{"generated":{"line":92,"column":3},"source":"assets/js/tooltip.js","original":{"line":59,"column":4}},{"generated":{"line":93,"column":0},"source":"assets/js/tooltip.js","original":{"line":87,"column":1}}],"sources":{"assets/js/tooltip.js":"import { css } from './utils'\r\n\r\nconst template = (data) => `\r\n    <div class=\"tooltip-title\">${data.title}</div>\r\n    <div class=\"tooltip-price\">${data.price}</div>\r\n`\r\n\r\nexport function tooltip($el){\r\n    const clear = () => { $el.innerHTML = '' }\r\n    return {\r\n        show( {left, top}, data ) {\r\n            const { height, width } = $el.getBoundingClientRect()\r\n            clear()\r\n            css($el, {\r\n                display: 'block',\r\n                top: top + height - 20 + 'px',\r\n                left: left + width / 3 + 'px'\r\n            })\r\n            $el.insertAdjacentHTML( 'afterbegin', template(data) )\r\n        },\r\n        hide() {\r\n            css($el, { display: 'none' })\r\n        }\r\n    }\r\n}\r\n\r\n\r\n/***************************** ------------- *****************************/\r\n\r\n\r\nconst overlay_template = (data) => `\r\n    <div id=\"overlay-slick\" class=\"overlay-container__content__img\">\r\n        ${data.img.map( src => {\r\n            return `<div class=\"slick-item\">\r\n                        <img src=\"${src}\" alt=\"image\">\r\n                    </div>`\r\n        } ).join('/n')}\r\n    </div>\r\n    <div class=\"overlay-container__content__text\">\r\n        <div class=\"close-overlay-button\">\r\n            <button id=\"close-overlay-menu-button\" class=\"close-button\">Close Overlay Menu</button>\r\n        </div>\r\n        <h3>${data.title}</h3>\r\n        <div class=\"overlay-rating\">\r\n            ${data.price}\r\n        </div>\r\n        <p class=\"overlay-description\">${data.description}</p>\r\n        <div class=\"overlay-add-to-cart\">\r\n            <button type=\"submit\" class=\"el-form__button\">Добавить в корзину</button>\r\n        </div>\r\n    </div>\r\n`\r\n\r\n\r\nexport function overlay($el){\r\n    const bg = $el.querySelector('.overlay-container__bg')\r\n    const content = $el.querySelector('.overlay-container__content')\r\n    const clear = () => { content.innerHTML = '' }\r\n    return {\r\n        show( data ){\r\n            clear()\r\n            css($el, {\r\n                display: 'block'\r\n            })\r\n            setTimeout( () => {\r\n                css( bg, {\r\n                    opacity: 1\r\n                } )\r\n                css( content, {\r\n                    opacity: 1,\r\n                    transform: 'scale(1) translate(-50%, -50%)'\r\n                } )\r\n            }, 200 )\r\n            content.insertAdjacentHTML( 'afterbegin', overlay_template(data) )\r\n        },\r\n        hide(){\r\n            css( content, {\r\n                opacity: 0,\r\n                transform: 'scale(0.8) translate(-50%, -50%)'\r\n            } )\r\n            css( bg, {\r\n                opacity: 0\r\n            } )\r\n            setTimeout( () => css($el, { display: 'none' }), 200 )\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"15cbbcfd42e9969e6941f4eac24a1a62","cacheData":{"env":{}}}